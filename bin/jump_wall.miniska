var IO = import "bind:std.native.io.log";
var Parameters = import "std:std.function.parameters";

var Character = import "character";

var run = function(origin: Character::Fcty(), parameters: Parameters::Fcty()) {
	var jumpDirection = parameters.asInt(0);
	var blockId = parameters.asInt(1);
	var emptyCallback = function(){};

	origin.input.remove();
	origin.move(jumpDirection, 30, 1, emptyCallback);
	IO.wait(400);
	origin.jump(14);
	origin.move(jumpDirection, 60, 1, origin.input.restore);
};

